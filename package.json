{
  "name": "yata-fetch",
  "version": "1.1.2",
  "description": "Adds CLI interface for importing translation files from Yata",
  "license": "MIT",
  "main": "./dist/yata",
  "author": "Dominik Zborowski <dominik.zborowski@gmail.com>",
  "keywords": [
    "yata",
    "translations",
    "i18n",
    "locale",
    "yatapp"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/dzbo/yata-fetch.git"
  },
  "bin": {
    "yata-fetch": "./bin/yata-fetch.js"
  },
  "scripts": {
    "start": "./node_modules/.bin/npm-run-all build server",
    "s": "./node_modules/.bin/npm-run-all build server",
    "server": "NODE_ENV=development webpack-dev-server --mode development",
    "build": "NODE_ENV=development webpack --mode development",
    "build:production": "NODE_ENV=production webpack --mode production",
    "lint:js": "./node_modules/.bin/eslint src/ tests/ --fix",
    "test": "./node_modules/.bin/karma start",
    "generate": "node bin/yata-fetch.js --config test/fixtures/yata.json"
  },
  "dependencies": {
    "nconf": "^0.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "eslint": "^5.14.1",
    "eslint-config-recommended": "^4.0.0",
    "eslint-loader": "^2.1.2",
    "karma": "^4.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^2.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.5",
    "mocha": "^6.0.2",
    "mock-fs": "^4.8.0",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  }
}
